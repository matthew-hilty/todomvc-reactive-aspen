{
  "name": "todomvc-reactive-aspen",
  "version": "0.0.0",
  "private": true,
  "description": "TodoMVC structured with the Reactive-Aspen framework",
  "engines": {
    "node": "~0.10.24",
    "npm": "2.3.x"
  },
  "main": "lib/initializer.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/matthew-hilty/todomvc-reactive-aspen.git"
  },
  "scripts": {
    "build": "npm run build-output && npm run compile",
    "build-output": "browserify -t coughify --extension='.cough' src/initializer.cough | bundle-collapser - | uglifyjs -cm -o public/output.js",
    "compile": "cough --no-header -o lib/ -bc src/",
    "start": "node server.js"
  },
  "keywords": [
    "todomvc",
    "reactive-aspen"
  ],
  "author": "Matthew Hilty",
  "license": "MIT",
  "dependencies": {
    "express": "~4.11.1",
    "cookie-parser": "~1.3.3",
    "pando": "0.0.4",
    "reactive-aspen": "0.0.2",
    "react-bridge": "0.0.2",
    "react": "~0.12.2"
  },
  "devDependencies": {
    "cough-syrup": "0.0.1",
    "coughify": "0.0.0",
    "uglify-js": "~2.4.16",
    "bundle-collapser": "~1.1.1",
    "browserify": "~8.1.1"
  }
}
